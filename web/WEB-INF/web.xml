<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Logar</servlet-name>
        <servlet-class>br.senai.Logar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cadastrar</servlet-name>
        <servlet-class>br.senai.Cadastrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastroServlet</servlet-name>
        <servlet-class>br.senai.servlets.CadastroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>br.senai.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TicketsSevlet</servlet-name>
        <servlet-class>br.senai.servlets.TicketsSevlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>br.senai.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>br.senai.servlets.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PerfilSevlet</servlet-name>
        <servlet-class>br.senai.servlets.PerfilSevlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logar</servlet-name>
        <url-pattern>/Logar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cadastrar</servlet-name>
        <url-pattern>/Cadastrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroServlet</servlet-name>
        <url-pattern>/CadastroServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TicketsSevlet</servlet-name>
        <url-pattern>/TicketsSevlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/AdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PerfilSevlet</servlet-name>
        <url-pattern>/PerfilSevlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
